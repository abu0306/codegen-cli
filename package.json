{
  "name": "codegen-cli",
  "version": "1.0.0",
  "description": "Tauri React 项目脚手架工具",
  "main": "src/index.js",
  "bin": {
    "codegen": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "pkg ."
  },
  "pkg": {
    "targets": [
      "node20-macos-x64",
      "node20-macos-arm64",
      "node20-linux-x64",
      "node20-win-x64"
