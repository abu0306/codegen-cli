    "outputPath": "dist"
  },
  "keywords": [
    "tauri",
    "react",
    "cli",
    "scaffold"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}